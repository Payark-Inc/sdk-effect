{
    "name": "@payark/sdk-effect",
    "version": "0.1.0",
    "description": "Functional, type-safe Effect SDK for the PayArk API.",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    },
    "files": [
        "dist",
        "README.md"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "test": "bun test",
        "lint": "tsc --noEmit",
        "prepublishOnly": "bun run build"
    },
    "keywords": [
        "payark",
        "payment",
        "effect",
        "functional",
        "typescript",
        "sdk"
    ],
    "author": "PayArk",
    "license": "MIT",
    "devDependencies": {
        "tsup": "^8.0.0",
        "typescript": "^5.7.0",
        "@types/node": "^20.0.0"
    },
    "dependencies": {
        "@effect/platform": "^0.94.5",
        "@effect/platform-node": "^0.104.1",
        "@effect/schema": "^0.75.5",
        "@payark/sdk": "^0.1.1",
        "effect": "^3.19.18"
    }
}